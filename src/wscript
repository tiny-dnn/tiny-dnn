def build(bld):
  bld(features = 'cxx cprogram',
      source = 'main.cpp',
      target = 'main',
      cflags   = ['-Wall'],
      cxxflags = ['-std=c++0x', '-Wall', '-s', '-O2', '-march=native'],
      lib      = ['stdc++'],
      includes = ['.', '../include', 'C:/Program Files/boost/boost_1_51_0'])
